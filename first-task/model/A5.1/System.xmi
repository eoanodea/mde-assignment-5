<?xml version="1.0" encoding="UTF-8"?>
<A2:System xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:A2="A2"
    version="1"
    subversion="1">
  <entities
      name="User">
    <features
        xsi:type="A2:Attribute"
        name="ID"
        IsPrimaryKey="true"/>
    <features
        xsi:type="A2:Attribute"
        name="username"/>
    <features
        xsi:type="A2:Attribute"
        name="password"
        type="string"/>
  </entities>
  <entities
      name="User Address">
    <features
        xsi:type="A2:Attribute"
        name="ID"
        IsPrimaryKey="true"/>
    <features
        xsi:type="A2:Attribute"
        name="House Number"/>
    <features
        xsi:type="A2:Attribute"
        name="Address Line 1"
        type="string"/>
    <features
        xsi:type="A2:Attribute"
        name="City"
        type="string"/>
    <features
        xsi:type="A2:Reference"
        name="userId (Foreign Key)"
        foreignKey="//@entities.0"/>
  </entities>
  <pages name="Home"
      identifier="1"
      url="website.com"
      isHomePage="true">
    <content
        xsi:type="A2:StaticContent"
        identifier="4"
        content="Sign up for our website"/>
    <content
        xsi:type="A2:Form"
        name="Sign up"
        identifier="5"
        entity="//@entities.0">
      <elements
          name="username"
          label="Enter username"
          tooltip="Enter username"
          attr="//@entities.0/@features.1"/>
      <elements
          name="Password"
          label="Password"
          tooltip="Password"
          attr="//@entities.0/@features.2"/>
    </content>
  </pages>
  <pages name="Profile"
      identifier="2"
      url="website.com/profile">
    <content
        xsi:type="A2:StaticContent"
        identifier="6"
        content="Profile page"/>
    <content
        xsi:type="A2:DynamicContent"
        identifier="8"
        entity="//@entities.0"
        attr="//@entities.0/@features.1"/>
    <content
        xsi:type="A2:DynamicContent"
        identifier="7"
        entity="//@entities.0"
        attr="//@entities.0/@features.2"/>
  </pages>
</A2:System>
